package pageobjects;

import org.openqa.selenium.By;

public class HomePage {

	public By FragranceXTitle = By.xpath("//title[contains(text(), 'Discount Perfume and Cologne - Free Shipping | FragranceX.com')]");
	public By TopPicksForYou = By.xpath("//*[@id=\"recommended-items\"]/div/div[1]//a[@href]");
	public By PerfumeVariants = By.xpath("/html/body/div[1]/section/div[1]/div[2]/div[1]/div[4]/div/div/div[2]/div/div/div[1]/h2");

}
